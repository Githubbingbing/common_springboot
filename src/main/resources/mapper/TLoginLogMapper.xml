<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboot.mapper.LoginLogMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.example.springboot.model.TLoginLog" id="tLoginLogMap">
        <result property="logId" column="log_id"/>
        <result property="logIp" column="log_ip"/>
        <result property="logUserId" column="log_user_id"/>
        <result property="loginName" column="login_name"/>
        <result property="loginTime" column="login_time"/>
    </resultMap>


</mapper>